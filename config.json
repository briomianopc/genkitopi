{
  "log": { "loglevel": "warning" },
  "inbounds": [
    {
      "port": PORT_PLACEHOLDER,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "UUID_PLACEHOLDER",
            "flow": "xtls-rprx-vision",
            "level": 0,
            "email": "koyeb@user"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "www.apple.com:443",
          "xver": 0,
          "serverNames": [
            "www.apple.com",
            "apple.com"
          ],
          "privateKey": "PRIVATE_KEY_PLACEHOLDER",
          "shortIds": [
            "SHORT_ID_PLACEHOLDER"
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    }
  ],
  "outbounds": [
    { "protocol": "freedom", "tag": "direct" },
    { "protocol": "blackhole", "tag": "block" }
  ]
